<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <appenders>
        <Console name="CONSOLE" target="system_out">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS,GMT+8} %highlight{[%p]} %c:%line - %m%n"/>
        </Console>
    </appenders>
    <loggers>
        <logger name="org.apache.zookeeper" level="error"/>
        <root level="info">
            <appenderref ref="CONSOLE"/>
        </root>
    </loggers>
</configuration>